		"@tanstack/react-query": "^5.90.2",
		"axios": "^1.12.2",
		"lodash": "^4.17.21",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"recharts": "^2.15.4",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.37.0",
		"@tanstack/eslint-plugin-query": "^5.91.0",
		"@types/lodash": "^4.17.20",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.1",
		"@vitejs/plugin-react": "^5.0.4",
