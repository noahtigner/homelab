{
	"name": "dashboard",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host 0.0.0.0",
		"build": "tsc && vite build",
		"format": "prettier --check . --config ../.prettierrc.json --ignore-path ../.prettierignore --ignore-path ../.gitignore",
		"format:fix": "prettier --write . --config ../.prettierrc.json --ignore-path ../.prettierignore --ignore-path ../.gitignore",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"typecheck": "tsc --noEmit",
		"preview": "vite preview"
	},
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.15.11",
		"@mui/material": "^5.15.6",
		"@tanstack/react-query": "^5.18.1",
		"axios": "^1.6.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"recharts": "^2.12.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^5.18.1",
		"@types/react": "^18.2.62",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@vitejs/plugin-react": "^4.2.1",
		"dotenv": "^16.4.1",
		"dotenv-expand": "^10.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3",
		"validate-env-vars": "^0.3.11",
		"vite": "^5.0.12"
	}
}
